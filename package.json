{
  "name": "treeo2-rest-service",
  "version": "0.0.1",
  "description": "A restful service for treeo",
  "author": "Fairventures Worldwide",
  "private": true,
  "license": "Apache 2.0",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "concurrently \"rimraf dist\" \"nest start --watch\" \"docker-compose up -d\"",
    "start:hmr": "concurrently \"rimraf dist\" \"nest build --webpack --webpackPath webpack-hmr.config.js --unhandled-rejections=strict --watch\" \"docker-compose up -d\"",
    "start:debug": "nest start --debug",
    "start:prod": "npm run prebuild && npm run build && npm install && node dist/src/main",
    "start:docker": "docker-compose down --remove-orphans; docker-compose up -d",
    "restart": "npm run prepare; docker-compose down --remove-orphans && npm run start:dev",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --force-exit --detectOpenHandles",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:actions": "npm run db:migrate; npm run db:seed; jest --config ./jest-e2e.json --coverage --runInBand --force-exit",
    "test:e2e": "rm sequelizeMeta.json; docker-compose down; docker-compose build; npm run prebuild; npm run build; sleep 2; docker-compose up -d; sleep 35; npm run db:migrate; npm run db:seed; jest --config ./jest-e2e.json --coverage --runInBand --force-exit",
    "db:migrate": "node_modules/.bin/sequelize db:migrate",
    "db:migrate:seed": "rm sequelizeMeta.json && docker-compose down; docker-compose up -d; sleep 30; npm run db:migrate; npm run db:seed",
    "db:rollback": "node_modules/.bin/sequelize db:migrate:undo:all",
    "db:rollmigrate": "npm run db:rollback && npm run db:migrate",
    "db:rollmigrateseed": "npm run db:rollback && npm run db:migrate && npm run db:seed",
    "db:undo": "node_modules/.bin/sequelize db:migrate:undo",
    "db:seed": "node_modules/.bin/sequelize db:seed:all",
    "db:seed:undo": "node_modules/.bin/sequelize db:seed:undo:all",
    "models:dev": "node_modules/.bin/sequelize-auto -h localhost -d postgres -u postgres -p 5438 -x example -e postgres -o \"./database/models\" ",
    "prepare": "husky install"
  },
  "dependencies": {
    "@apollo/gateway": "^0.46.0",
    "@nestjs-modules/mailer": "^1.6.1",
    "@nestjs/apollo": "^10.0.8",
    "@nestjs/axios": "^0.0.7",
    "@nestjs/bull": "^0.5.4",
    "@nestjs/common": "^8.4.3",
    "@nestjs/config": "^2.0.0",
    "@nestjs/core": "^8.4.3",
    "@nestjs/event-emitter": "^1.1.0",
    "@nestjs/graphql": "^10.0.8",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-express": "^8.4.3",
    "@nestjs/sequelize": "^8.0.0",
    "@nestjs/swagger": "^5.2.1",
    "@splitsoftware/splitio": "^10.16.0",
    "@types/bull": "^3.15.0",
    "@types/passport-facebook": "^2.1.10",
    "apollo-server-core": "^3.6.7",
    "apollo-server-express": "^3.6.7",
    "axios": "^0.26.1",
    "bcrypt": "^5.0.0",
    "bull": "^3.20.1",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "crypto": "^1.0.1",
    "csurf": "^1.11.0",
    "dotenv": "^8.2.0",
    "form-data": "^4.0.0",
    "google-auth-library": "^7.0.2",
    "graphql": "^15.8.0",
    "graphql-tools": "^7.0.5",
    "graphql-type-json": "^0.3.2",
    "helmet": "^4.4.1",
    "i": "^0.3.7",
    "ioredis": "^4.22.0",
    "libphonenumber-js": "^1.9.17",
    "nodemailer": "^6.7.2",
    "npm": "^8.0.0",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-facebook-token": "^4.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "redis": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.5",
    "sequelize": "6.6.2",
    "sequelize-auto": "^0.8.3",
    "sequelize-cli": "^6.2.0",
    "sequelize-typescript": "2.1.0",
    "swagger-ui-express": "^4.1.6",
    "ts-morph": "^13.0.3",
    "twilio": "^3.75.1",
    "uuid": "^8.3.2",
    "winston": "^3.6.0",
    "yup": "^0.32.8"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.1.2",
    "@nestjs/cli": "^8.2.2",
    "@nestjs/schematics": "^8.0.10",
    "@nestjs/testing": "^8.4.3",
    "@types/axios": "^0.14.0",
    "@types/bcrypt": "^3.0.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.8",
    "@types/ioredis": "^4.19.4",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.6",
    "@types/nodemailer": "^6.4.0",
    "@types/passport-jwt": "^3.0.4",
    "@types/passport-local": "^1.0.33",
    "@types/pg": "^7.14.9",
    "@types/sequelize": "^4.28.9",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.3.0",
    "@types/yup": "^0.29.11",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "concurrently": "^5.3.0",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^5.0.9",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "prettier": "^2.1.2",
    "run-script-webpack-plugin": "^0.0.11",
    "supertest": "^6.0.0",
    "ts-jest": "^26.4.3",
    "ts-loader": "^8.0.8",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.5",
    "webpack": "^5.72.0",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      ".module.ts$"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js, ts}": [
      "npm run format",
      "git add"
    ]
  }
}
